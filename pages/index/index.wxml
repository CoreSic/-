<!-- 搜索栏 -->
<view class='SousuoArea'>
  <input type='emoji' placeholder="请输入设备名" bindinput="getShebei"></input>
  <button class="Sousuo-button" size="mini" bindtap="goToSousuo">搜索</button>
</view>
<!-- 公告栏 -->
<view class="function-index-text">
  <view wx:if="{{result!=''}}">
    <text>{{result}}</text> 
  </view>
  <text wx:else>公告：系统升级中，敬请期待....</text>
</view>
<!-- 首页功能区 -->
<view class='function-index'>
  <view class="function-index-box">
    <navigator class="" target="" url="/pages/guide/guide" hover-class="navigator-hover" open-type="navigate">
      <image src="/images/index/zhinan.png"></image>
    </navigator>
    
    
    <text>使用指南</text>
  </view>
  <view class="function-index-box">
    <image src="/images/index/sao.png" bindtap="getScancode"></image>
    <text>扫一扫</text>
  </view>
</view>
